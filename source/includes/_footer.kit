<script>
    $( document ).ready(function() {
        var allCookies = document.cookie.split('; ');
        var mycookies = Array();

        for ( var i = 0; i < allCookies.length; i++) {
            var cookiePair = allCookies[i].split('=');
            mycookies[cookiePair[0]] = cookiePair[1];
        }
        if (!mycookies['banner-clicked'])
        {
            $('.cwb').show();
            $('body').addClass("banner-visible");
        }

        $("#banner-confirm-button").click(function(){
            var d = new Date();
            d.setTime(d.getTime() + 2*365*24*60*60*1000); // in milliseconds
            document.cookie="banner-clicked=true; expires="+d.toGMTString()+"; path=/";
            $('.cwb').hide();
            $('body').removeClass("banner-visible");
        })
    });
</script>


